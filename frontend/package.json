{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "generate": "openapi --input http://localhost:15557/api/docs/openapi.json --indent 2 --output ./src/client/api"
  },
  "dependencies": {
    "@vueuse/core": "^10.9.0",
    "ansi_up": "^6.0.2",
    "daisyui": "^4.7.2",
    "echarts": "^5.5.0",
    "material-symbols": "^0.16.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.10",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "npm-run-all2": "^6.1.1",
    "openapi-typescript-codegen": "^0.28.0",
    "postcss-import": "^16.0.1",
    "postcss-nesting": "^13.0.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.0",
    "uuid": "^10.0.0",
    "vite": "^5.0.11",
    "vue-tsc": "^1.8.27"
  }
}
